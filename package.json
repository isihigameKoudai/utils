{
  "name": "utils-ts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint '{src,utils}/**/*.{ts,tsx}' --fix",
    "format": "prettier --write '{src,utils}/**/*.{ts,tsx}'",
    "claude": "npx tsx script/claude.ts",
    "generate:cursorrules": "node script/generate-cursor-rules.js \"**/*.ai.md\" \".cursor/rules/main.mdc\"",
    "tsr": "npx tsr -w 'src/main\\.ts$' '.*\\.test\\.ts$'",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@tanstack/react-router": "^1.132.47",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow-models/face-detection": "^1.0.3",
    "@tensorflow-models/face-landmarks-detection": "^1.0.6",
    "@tensorflow-models/hand-pose-detection": "^2.0.1",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "4.22",
    "@tensorflow/tfjs-backend-webgpu": "4.22",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@types/lodash-es": "^4.17.12",
    "dat.gui": "^0.7.9",
    "dayjs": "^1.11.18",
    "lightweight-charts": "^5.0.9",
    "lodash-es": "^4.17.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.5.1",
    "three": "^0.180.0",
    "typestyle": "^2.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.0.14",
    "@tanstack/router-devtools": "^1.132.51",
    "@tanstack/router-plugin": "^1.132.51",
    "@types/dat.gui": "^0.7.13",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-pages": "^0.33.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "{src,utils}/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
